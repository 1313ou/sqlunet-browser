<?xml version="1.0"?>
<!--suppress XmlPathReference, XmlPathReference -->
<xs:schema xmlns:xs='http://www.w3.org/2001/XMLSchema'
  targetNamespace='http://org.sqlunet'
  xmlns='http://org.sqlunet'
  xmlns:wn='http://org.sqlunet/wn'
  xmlns:vn='http://org.sqlunet/vn'
  xmlns:pb='http://org.sqlunet/pb'
>

<xs:import namespace="http://org.sqlunet/wn" schemaLocation="WordNet.xsd"/>
<xs:import namespace="http://org.sqlunet/vn" schemaLocation="VerbNet.xsd"/>
<xs:import namespace="http://org.sqlunet/pb" schemaLocation="PropBank.xsd"/>

 <xs:element name='sqlunet'>
  <xs:complexType>
   <xs:sequence>
    <xs:element ref='wn:wordnet' minOccurs='0' maxOccurs='1'/>
    <xs:element ref='vn:verbnet' minOccurs='0' maxOccurs='1'/>
    <xs:element ref='pb:propbank' minOccurs='0' maxOccurs='1'/>
  </xs:sequence>
  </xs:complexType>
 </xs:element>

</xs:schema>
