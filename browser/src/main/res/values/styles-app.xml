	<!-- T A R G E T V I E W -->

	<style name="Query">
		<item name="android:layout_width">match_parent</item>
		<item name="android:layout_height">1dp</item>
		<item name="android:paddingStart">@dimen/listitem_horizontal_padding</item>
		<item name="android:paddingEnd">@dimen/listitem_horizontal_padding</item>
		<item name="android:paddingTop">@dimen/listitem_vertical_padding</item>
		<item name="android:paddingBottom">@dimen/listitem_vertical_padding</item>
		<item name="android:textSize">12sp</item>
		<item name="android:textColor">@android:color/white</item>
		<item name="android:background">@android:color/holo_green_light</item>
	</style>

	<!-- L I S T -->

	<style name="ListItemColumn">
		<item name="android:layout_width">0dp</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:paddingStart">@dimen/listitem_horizontal_padding</item>
		<item name="android:paddingEnd">@dimen/listitem_horizontal_padding</item>
		<item name="android:paddingTop">@dimen/listitem_vertical_padding</item>
		<item name="android:paddingBottom">@dimen/listitem_vertical_padding</item>
		<item name="android:gravity">center_vertical</item>
		<item name="android:textIsSelectable">true</item>
		<item name="android:textColor">@color/dark_gray_color</item>
		<item name="android:textAppearance">?android:attr/textAppearanceMedium</item>
		<item name="android:ellipsize">end</item>
	</style>

	<style name="ListItemColumn.Small">
		<item name="android:textAppearance">?android:attr/textAppearanceListItemSmall</item>
		<item name="android:textColor">@color/middle_gray_color</item>
	</style>

	<style name="ListItemColumn.Enhanced">
		<item name="android:background">@color/light_gray_color</item>
	</style>

	<style name="ListItem">
		<item name="android:layout_width">match_parent</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:layout_marginBottom">@dimen/list_vertical_margin</item>
		<item name="android:layout_marginTop">@dimen/list_vertical_margin</item>
		<item name="android:layout_marginLeft">@dimen/list_horizontal_margin</item>
		<item name="android:layout_marginRight">@dimen/list_horizontal_margin</item>
		<item name="android:background">@drawable/rounded_corners_white</item>
		<item name="android:gravity">center_vertical</item>
		<item name="android:paddingTop">@dimen/list_vertical_padding</item>
		<item name="android:paddingBottom">@dimen/list_vertical_padding</item>
		<item name="android:paddingLeft">@dimen/list_horizontal_padding</item>
		<item name="android:paddingRight">@dimen/list_horizontal_padding</item>
		<item name="android:textIsSelectable">true</item>
		<item name="android:descendantFocusability">blocksDescendants</item>
	</style>

	<style name="ListItem1">
		<item name="android:layout_width">match_parent</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:textAppearance">?android:attr/textAppearanceListItemSmall</item>
		<item name="android:gravity">center_vertical</item>
		<item name="android:paddingLeft">?android:attr/listPreferredItemPaddingStart</item>
		<item name="android:paddingRight">?android:attr/listPreferredItemPaddingEnd</item>
		<item name="android:textIsSelectable">true</item>
		<item name="android:minHeight">?android:attr/listPreferredItemHeightSmall</item>
	</style>

	<!-- S T A T U S   /   S E T U P  -->

	<style name="GridLabel">
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:layout_gravity">center_vertical|end</item>
		<item name="android:layout_marginLeft">20dp</item>
		<item name="android:gravity">end</item>
		<item name="android:maxLines">2</item>
	</style>

	<style name="GridData">
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:orientation">horizontal</item>
		<item name="android:paddingLeft">20dp</item>
		<item name="android:paddingRight">20dp</item>Z
	</style>

	<style name="GridStatusImage">
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:layout_gravity">center</item>
		<item name="android:layout_marginRight">10dp</item>
		<item name="android:background">@color/light_yellow_color</item>
		<item name="android:paddingBottom">12dp</item>
		<item name="android:paddingTop">12dp</item>
		<item name="android:paddingLeft">20dp</item>
		<item name="android:paddingRight">20dp</item>
		<item name="android:src">@drawable/ic_unknown</item>
	</style>

	<style name="GridStatusActionButton">
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:layout_gravity">center</item>
		<item name="android:layout_marginLeft">8dp</item>
		<item name="android:layout_marginRight">8dp</item>
		<item name="android:paddingTop">2dp</item>
		<item name="android:paddingBottom">2dp</item>
		<item name="android:background">@drawable/bg_button_action</item>
		<item name="android:gravity">end|center_vertical</item>
		<item name="android:visibility">gone</item>
	</style>

	<style name="GridStatusInfoButton">
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:layout_gravity">center</item>
		<item name="android:layout_marginLeft">8dp</item>
		<item name="android:layout_marginRight">8dp</item>
		<item name="android:paddingTop">2dp</item>
		<item name="android:paddingBottom">2dp</item>
		<item name="android:background">@drawable/bg_button_info</item>
		<item name="android:gravity">end|center_vertical</item>
		<item name="android:visibility">visible</item>
	</style>

	<style name="GridActionButton">
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:layout_gravity">center</item>
		<item name="android:layout_marginLeft">8dp</item>
		<item name="android:layout_marginRight">8dp</item>
		<item name="android:paddingTop">2dp</item>
		<item name="android:paddingBottom">2dp</item>
		<item name="android:background">@drawable/bg_button_action</item>
		<item name="android:gravity">center</item>
		<item name="android:visibility">visible</item>
	</style>

	<style name="GridInfoButton">
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:layout_gravity">center</item>
		<item name="android:layout_marginLeft">8dp</item>
		<item name="android:layout_marginRight">8dp</item>
		<item name="android:paddingTop">2dp</item>
		<item name="android:paddingBottom">2dp</item>
		<item name="android:background">@drawable/bg_button_info</item>
		<item name="android:gravity">end|center_vertical</item>
		<item name="android:visibility">visible</item>
	</style>

</resources>