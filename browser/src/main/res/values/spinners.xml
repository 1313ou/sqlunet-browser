<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">

	<!-- mode spinner
	SELECTORS
	XSELECTORS
	-->
	<string-array name="selectors_names" tools:ignore="InconsistentArrays">
		<item>senses</item>
		<item>grouped by source</item>
	</string-array>
	<integer-array name="selectors_icons" tools:ignore="InconsistentArrays">
		<item>@drawable/ic_selector</item>
		<item>@drawable/ic_xselector</item>
	</integer-array>

	<!-- textsearch spinner
	WORDS
	DEFINITIONS
	SAMPLES
	VERBNET EXAMPLES
	PROPBANK EXAMPLES
	FRAMENET SENTENCES
	-->
	<string-array name="textsearch_modes" tools:ignore="InconsistentArrays">
		<item>WordNet definitions</item>
		<item>WordNet samples</item>
		<item>WordNet words</item>
		<item>VerbNet examples</item>
		<item>ProbBank examples</item>
		<item>FrameNet sentences</item>
	</string-array>
	<integer-array name="textsearch_icons" tools:ignore="InconsistentArrays">
		<item>@drawable/ic_search_wndefinition</item>
		<item>@drawable/ic_search_wnsample</item>
		<item>@drawable/ic_search_wnword</item>
		<item>@drawable/ic_search_vnexample</item>
		<item>@drawable/ic_search_pbexample</item>
		<item>@drawable/ic_search_fnsentence</item>
	</integer-array>

	<!-- predicate matrix spinner
	ROLES
	ROWS_GROUPED_BY_ROLE
	ROWS_GROUPED_BY_SYNSET
	ROWS
	-->
	<string-array name="predicatematrix_modes">
		<item>grouped</item>
		<item>rows by role</item>
		<item>rows by synset</item>
		<item>rows</item>
	</string-array>
	<integer-array name="predicatematrix_icons">
		<item>@drawable/ic_roles_grouped</item>
		<item>@drawable/ic_rows_byrole</item>
		<item>@drawable/ic_rows_bysynset</item>
		<item>@drawable/ic_rows_ungrouped</item>
	</integer-array>

</resources>