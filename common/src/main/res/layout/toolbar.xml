<?xml version="1.0" encoding="utf-8"?>
<androidx.appcompat.widget.Toolbar xmlns:android="http://schemas.android.com/apk/res/android"
                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                   xmlns:tools="http://schemas.android.com/tools"
                                   android:id="@+id/toolbar"
                                   android:layout_width="match_parent"
                                   android:layout_height="?attr/actionBarSize"
                                   android:theme="@style/CompatAppTheme.AppBarOverlay"
                                   app:elevation="4dp"
                                   app:title="@string/app_name"
                                   app:subtitle="@string/app_subname"
                                   app:popupTheme="@style/CompatAppTheme.PopupOverlay"
                                   app:subtitleTextColor="@color/primaryAltForeColor"
                                   app:titleTextColor="@color/primaryForeColor"
                                   tools:ignore="UnusedAttribute"/>