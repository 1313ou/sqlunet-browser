<?xml version="1.0" encoding="UTF-8"?>

<link
	name="Caused"
	id="caused"
	type="semantic"
	v="true">

	<img src="caused.png" />

	<description>
		<span>Is caused by</span>
	</description>

	<logical>
		<def>
			<span>
				A verb
				<b>Y</b>
				is caused by
				<b>X</b>
			</span>
		</def>
		<cond>
			<span>
				<b>X</b>
				denotes the causation of the state or activity referred to by
				<b>Y</b>
			</span>
		</cond>
	</logical>

	<examples>
		<example>
			<from>have</from>
			<to>give</to>
			<gloss>resultative</gloss>
		</example>
		<example>
			<from>see</from>
			<to>show</to>
			<gloss>resultative</gloss>
		</example>
		<example>
			<from>rise</from>
			<to>raise</to>
			<gloss>resultative</gloss>
		</example>
		<example>
			<from>learn</from>
			<to>teach</to>
			<gloss>resultative</gloss>
		</example>
		<example>
			<from>eat</from>
			<to>feed</to>
			<gloss>resultative</gloss>
		</example>
		<example>
			<from>fear</from>
			<to>scare</to>
			<gloss>resultative</gloss>
		</example>
		<example>
			<from>fall</from>
			<to>fell</to>
			<gloss>resultative</gloss>
		</example>
	</examples>

	<see tag="entailed" />

</link>

