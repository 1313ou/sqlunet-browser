<link
	name="Hypernym"
	id="hypernym"
	type="semantic"
	reverse="hyponym"
	n="true"
	v="true">

	<img src="hypernym.png" />

	<description>
		<span>Superordinate (i.e. more generic). The generic term used to designate a whole class of specific instances.</span>
	</description>

	<logical>
		<def>
			<span>
				<b>X</b>
				is a hypernym of
				<b>Y</b>
			</span>
		</def>
		<cond>
			<span>
				<b>Y</b>
				is a (kind of)
				<b>X</b>
				<i>(nouns)</i>
			</span>
		</cond>
		<cond>
			<span>
				to
				<b>Y</b>
				is to
				<b>X</b>
				in some particular manner
				<i>(verbs)</i>
			</span>
		</cond>
	</logical>

	<examples>
		<example>
			<from>oak</from>
			<to>tree</to>
			<gloss>an oak is a kind of tree</gloss>
		</example>
		<example>
			<from>tree</from>
			<to>plant</to>
			<gloss>a tree is a kind of plant</gloss>
		</example>
		<example>
			<from>house</from>
			<to>dwelling</to>
			<gloss>a house is a kind of dwelling</gloss>
		</example>
		<example>
			<from>disappointment</from>
			<to>disatisfaction</to>
			<gloss>disappointment is a kind of disatisfaction</gloss>
		</example>
		<example>
			<from>amble</from>
			<to>walk</to>
			<gloss>to amble is to walk in some manner</gloss>
		</example>
		<example>
			<from>march</from>
			<to>walk</to>
			<gloss>to march is to walk in some manner</gloss>
		</example>
		<example>
			<from>lisp</from>
			<to>say</to>
			<gloss>to lisp is to say in some manner</gloss>
		</example>
		<example>
			<from>slam</from>
			<to>hit</to>
			<gloss>to slam is to hit in some manner</gloss>
		</example>

		<example>
			<from>Dien Bien Phu</from>
			<to>siege</to>
			<gloss>instance hypernym</gloss>
			<img src="instance.hypernym.png" />
		</example>
		<example>
			<from>Korean war</from>
			<to>war</to>
			<gloss>instance hypernym</gloss>
			<img src="instance.hypernym.png" />
		</example>
		<example>
			<from>Chartres cathedral</from>
			<to>cathedral</to>
			<gloss>instance hypernym</gloss>
			<img src="instance.hypernym.png" />
		</example>
		<example>
			<from>Carnegie Mellon University</from>
			<to>university</to>
			<gloss>instance hypernym</gloss>
			<img src="instance.hypernym.png" />
		</example>
		<example>
			<from>Mayflower</from>
			<to>ship</to>
			<gloss>instance hypernym</gloss>
			<img src="instance.hypernym.png" />
		</example>
	</examples>
	>

	<see tag="hyponym">(the inverse relation for nouns)</see>
	<see tag="troponym">(the inverse relation for verbs)</see>

</link>

