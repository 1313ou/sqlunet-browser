<relation name="Entails" id="entails" type="semantic" inverse="entailed" v="true">

  <img src="entails.png"/>

  <description>
    <span>Entails</span>
  </description>

  <logical>
    <def><!-- @formatter:off --><span>A verb <b>A</b> entails <b>E</b></span><!-- @formatter:on --></def>
    <cond><!-- @formatter:off --><span><b>A</b> cannot be done unless <b>E</b> is, or has been, done</span><!-- @formatter:on --></cond>
    <cond><!-- @formatter:off --><span>there is no conceivable state of affairs that could make <b>A</b> true and <b>E</b> false</span><!-- @formatter:on --></cond>
  </logical>

  <phrases>
    <phrase><span>entails</span></phrase>
  </phrases>

  <guides>
    <guide>
      <from/>
      <to>entails</to>
      <gloss/>
    </guide>
    <guide>
      <from>A</from>
      <to>E</to>
      <gloss/>
    </guide>
  </guides>

  <examples>
    <example>
      <from>limp</from>
      <to>walk</to>
      <gloss>temporal inclusion - coextensive</gloss>
    </example>
    <example>
      <from>lisp</from>
      <to>talk</to>
      <gloss>temporal inclusion - coextensive</gloss>
    </example>
    <example>
      <from>snore</from>
      <to>sleep</to>
      <gloss>temporal inclusion - proper inclusion</gloss>
    </example>
    <example>
      <from>buy</from>
