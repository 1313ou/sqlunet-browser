<?xml version="1.0" encoding="UTF-8"?>

<link name="Holonym" id="holonym" type="semantic" n="true">

	<img src="holonym.png" />

	<description>
		<span>The name of the whole of which the meronym names a part, a member or the substance. Whole name (a word that names the whole of which a given word is a
			part).</span>
	</description>

	<logical>
		<def>
			<span>
				<b>X</b>
				is a holonym of
				<b>Y</b>
			</span>
		</def>
		<cond>
			<span>
				<b>Y</b>
				is a part of
				<b>X</b>
			</span>
		</cond>
		<cond>
			<span>
				<b>Y</b>
				is a substance of
				<b>X</b>
			</span>
		</cond>
		<cond>
			<span>
				<b>Y</b>
				is a member of
				<b>X</b>
			</span>
		</cond>
	</logical>

	<examples>
		<example>
			<from>wing</from>
			<to>bird</to>
			<gloss>component-object</gloss>
			<img src="part.holonym.png" />
		</example>
		<example>
			<from>mouth</from>
			<to>face</to>
			<gloss>component-object</gloss>
			<img src="part.holonym.png" />
		</example>
		<example>
			<from>arm</from>
			<to>body</to>
			<gloss>object-component</gloss>
			<img src="part.holonym.png" />
		</example>
		<example>
			<from>handle</from>
			<to>door</to>
			<gloss>object-component</gloss>
			<img src="part.holonym.png" />
		</example>
		<example>
			<from>branch</from>
			<to>tree</to>
			<gloss>component-object</gloss>
			<img src="part.holonym.png" />
		</example>
		<example>
			<from>tree</from>
			<to>forest</to>
			<gloss>member-collection</gloss>
			<img src="member.holonym.png" />
		</example>
		<example>
			<from>oxygen</from>
			<to>air</to>
			<gloss>substance-object</gloss>
			<img src="substance.holonym.png" />
		</example>
		<example>
			<from>olive</from>
			<to>olive oil</to>
			<gloss>substance-object</gloss>
			<img src="substance.holonym.png" />
		</example>
		<example>
			<from>adolescence</from>
			<to>growing up</to>
			<gloss>phase-process</gloss>
		</example>
		<example>
			<from>slice</from>
			<to>cake</to>
			<gloss>portion-mass</gloss>
		</example>
		<example>
			<from>paying</from>
			<to>shopping</to>
			<gloss>feature-activity</gloss>
		</example>
	</examples>

	<see tag="meronym">(the inverse relation)</see>

</link>
