<relation
	name="By Means Of"
	id="bymeansof"
	type="lexical"
	reflexive="false"
	v="true"
	n="true">

	<img src="bymeansof.png" />

	<description>
		<span>Derivational relation connecting verb and noun senses, with semantic type <strong>By Means Of</strong></span>
	</description>

	<examples>
		<example>
			<from>dilate</from>
			<to>dilator</to>
		</example>
		<example>
			<from>acidify</from>
			<to>acid</to>
		</example>
		<example>
			<from>argue</from>
			<to>argument</to>
		</example>
	</examples>

</relation>
