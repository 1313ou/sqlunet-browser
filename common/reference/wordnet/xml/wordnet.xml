<concept name="Examples" id="wnsample">
	<img src="wnsample.png" />
	<description>The example sentences or phrases of a synset</description>
	<examples>
		<context><name>[broadcast, circularise, circularize, circulate, diffuse, disperse, disseminate, distribute, pass around, propagate, spread]</name> synset </context>
		<example>spread information</example>
		<example>spread a rumor</example>
		<example>broadcast the news</example>
	</examples>
</concept>

<concept name="Morphological form" id="wnmorph">
	<img src="wnmorph.png" />
	<description>The morphological forms of a lemma</description>
	<examples>
		<context><name>mouse</name> lemma</context>
		<example>mice</example>
	</examples>
</concept>

<concept name="VerbFrame" id="wnverbframe">
	<img src="wnverbframe.png" />
	<description>Verb frames are the syntactic constructs attached to a sense of the verb.</description>
	<examples>
		<context><name>[broadcast, circularise, circularize, circulate, diffuse, disperse, disseminate, distribute, pass around, propagate, spread]</name> synset </context>
		<example>Somebody ___s something</example>
	</examples>
</concept>

<concept name="VerbTemplate" id="wnverbtemplate">
	<img src="wnverbtemplate.png" />
	<description>Verb templates or example sentences illustrate actual uses of the verb. They are attached to some verb senses.</description>
	<examples>
		<context><name>[flip, toss]</name> synset </context>
		<example>They ___ the coin.</example>
	</examples>
</concept>

<concept name="Adjective position" id="wnadjposition">
	<img src="wnadjposition.png" />
	<description>Adjective position. They are attached to some adjective senses,</description>
	<items>
		<item>predicative</item>
		<item>pre-nominal (attributive)</item>
		<item>immediately post-nominal</item>
	</items>
	<examples>
		<example>(p) lacking</example>
		<example>(a) putative</example>
		<example>(ip) galore</example>
	</examples>
</concept>

