<?xml version="1.0" encoding="UTF-8"?>
<concepts name="WordNet">

<concept name="Synset" id="wnsynset">
	<img src="wnsynset.png" />
	<description> Nouns, verbs, adjectives and adverbs are grouped into sets of cognitive synonyms (synsets), each expressing a distinct concept.</description>
	<examples>
		<example>[broadcast, circularise, circularize, circulate, diffuse, disperse, disseminate, distribute, pass around, propagate, spread]</example>
		<example>[circulate, go round, spread]</example>
		<example>[fear, fearfulness, fright]</example>
	</examples>
</concept>

<concept name="Members" id="wnmembers">
	<img src="wnmembers.png" />
	<description>Synsets have lemmas as members.</description>
	<examples>
		<context><name>[fear, fearfulness, fright]</name>synset</context>
		<example>[fear]</example>
		<example>[fearfulness]</example>
		<example>[fright]</example>
	</examples>
</concept>

<concept name="Member" id="wnmember">
	<img src="wnmember.png" />
	<description>A synset member is a lemma. </description>
	<examples>
		<context><name>[fear, fearfulness, fright]</name>synset</context>
		<example>fear</example>
	</examples>
</concept>

<concept name="Lemma" id="wnlemma">
	<img src="wnlemma.png" />
	<description>A lemma is a word in canonical form. </description>
	<examples>
		<example>jump</example>
	</examples>
</concept>

<concept name="Relation" id="wnrelation">
	<img src="wnrelation.png" />
	<a href="relations/index.html"/>
	<description>Synsets are interlinked by means of conceptual-semantic and lexical relations. WordNet labels the semantic relations among words, whereas the groupings of words in a thesaurus does not follow any explicit pattern other than meaning similarity.</description>
	<items>
		<item>antonym</item>
		<item>hypernym</item>
		<item>instance hypernym</item>
		<item>hyponym</item>
		<item>instance hyponym</item>
		<item>holonym member</item>
		<item>holonym substance</item>
		<item>holonym part</item>
		<item>meronym member</item>
		<item>meronym substance</item>
		<item>meronym part</item>
		<item>attribute</item>
		<item>pertainym</item>
		<item>derivation</item>
		<item>domain topic</item>
		<item>has domain topic</item>
		<item>domain region</item>
		<item>has domain region</item>
		<item>exemplifies</item>
		<item>is exemplified by</item>
		<item>also</item>
		<item>entails</item>
		<item>is entailed by</item>
		<item>similar</item>
		<item>similar</item>
		<item>participle</item>
		<item>causes</item>
		<item>is caused by</item>
	</items>
</concept>

